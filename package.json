{
  "name": "image-demo",
  "version": "1.0.2",
  "description": "🍎 🍪 🍦 🍓  基于image2D和image3D开发的图形用例。",
  "scripts": {
    "start": "npm run update && npm run dev",
    "dev": "cuf --server 30000",
    "update": "npm install &&  npm run copy-image2d && npm run copy-image3d",
    "copy-image2d": "cuf --copy node_modules/image2d/build/image2D.js lib/min/image2D.js",
    "copy-image3d": "cuf --copy node_modules/image3d/build/image3D.js lib/min/image3D.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/Image-Demo.git"
  },
  "keywords": [
    "algorithm",
    "image"
  ],
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/Image-Demo/issues"
  },
  "homepage": "https://github.com/yelloxing/Image-Demo#readme",
  "devDependencies": {
    "cuf": "^1.2.0"
  },
  "dependencies": {
    "image2d": "^1.6.0",
    "image3d": "^2.0.8"
  }
}
